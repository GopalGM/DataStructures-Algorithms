class Solution:
    def fib(self, N: int) -> int:
        '''
        Top down with Memoization
        '''
        top_down = {0 : 0 , 1 : 1}
        
        if N <= 1:
            return N
        
        if N in top_down:
            top_down[N] = N
        
        top_down[N] = self.fib(N-1) + self.fib(N-2)
        
        return top_down[N]
